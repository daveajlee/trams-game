<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="trams.data">

    <class name="RouteSchedule" table="ROUTESCHEDULE">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>
        <property name="routeNumber" column="ROUTE_NUMBER"/>
        <property name="scheduleNumber" column="SCHEDULE_NUMBER"/>
        <property name="delayInMins" column="DELAY_IN_MINS"/>
        <list name="serviceList" lazy="true" cascade="all-delete-orphan" table="SERVICELISTS" inverse="false"> 
        	<key column="SERVICELIST_ID"/> 
			<index column="POSITION_index"/> 
			<many-to-many unique="true" column="ID" class="Service"/> 
		</list>
    </class>

</hibernate-mapping>